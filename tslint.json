{
    "extends": "tslint-ionic-rules",
    "rules": {
        "no-trailing-whitespace": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ]
    }
}
